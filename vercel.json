{
  "functions": {
    "src/webhook_server.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/interactions",
      "dest": "/src/webhook_server.py"
    },
    {
      "src": "/health",
      "dest": "/src/webhook_server.py"
    }
  ],
  "crons": [
    {
      "path": "/scheduler",
      "schedule": "0 21 * * *"
    }
  ]
}